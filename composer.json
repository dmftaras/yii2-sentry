{
  "name": "dmftaras/yii2-sentry",
  "description": "Yii 2 extension for Sentry",
  "type": "yii2-extension",
  "keywords": ["yii2","extension","sentry"],
  "license": "MIT",
  "authors": [
    {
      "name": "Taras Kostetskyi",
      "email": "dmftaras@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "sentry/sentry": ">=2.1.0",
    "guzzlehttp/guzzle": "^7.0",
    "guzzlehttp/psr7": "^1.0",
    "php-http/guzzle7-adapter": "*",
    "php": ">=7.4.0"
  },
  "autoload": {
    "psr-4": {
      "dmftaras\\sentry\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    },
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
